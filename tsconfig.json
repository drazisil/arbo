{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "preserve",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noEmit": true,
    },
    "exclude": [
        "coverage",
        "node_modules",
    ],
}